div
    .telegram-logo
    .labels-container
        img.image(srcset="../images/Login_1@1x.png 1x, ../images/Login_1@2x.png 2x, ../images/Login_1@3x.png 3x")
        .header='Enter a Password'
        .notice='Account is protected with an additional password.'
    inputs-wrapper(class='phone')
        .password-container
            input(data-element-ref='password' type='password')
            input(type='checkbox' data-element-ref='togglePasswordVisibility')
    button-component(class='submit-form' data-input-title='buttonTitle' data-output-click='submit')

